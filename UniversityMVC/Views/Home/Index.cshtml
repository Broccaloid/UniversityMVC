@model IEnumerable<UniversityMVC.Models.Course>
@{
    ViewData["Title"] = "Home Page";
}

<h3>Смартфоны</h3>
<table class="table-condensed">
    <tr>
        <td>ID</td>
        <td>Название</td>
    </tr>
    @foreach (var course in Model)
    {
<tr>
    <td>@course.Id</td>
    <td><a href="~/Home/SelectCourse/@course.Id">@course.Name</a></td>
</tr>
    }
</table>
